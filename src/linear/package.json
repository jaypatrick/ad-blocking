{
  "name": "linear-import",
  "version": "1.0.0",
  "description": "Import documentation into Linear project management (Deno/Bun)",
  "type": "module",
  "scripts": {
    "bun:import": "bun run src/mod.ts",
    "bun:import:docs": "bun run src/mod.ts --file ../../docs/LINEAR_DOCUMENTATION.md",
    "bun:import:dry-run": "bun run src/mod.ts --file ../../docs/LINEAR_DOCUMENTATION.md --dry-run"
  },
  "keywords": [
    "linear",
    "documentation",
    "import",
    "project-management",
    "deno",
    "bun"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@linear/sdk": "^37.0.0",
    "dotenv": "^16.5.0",
    "marked": "^15.0.0",
    "commander": "^13.1.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
