{
  "name": "linear-import",
  "version": "1.0.0",
  "description": "Import documentation into Linear project management",
  "type": "module",
  "main": "dist/linear-import.js",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/linear-import.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "import": "node dist/linear-import.js",
    "import:docs": "node dist/linear-import.js --file ../../docs/LINEAR_DOCUMENTATION.md",
    "import:dry-run": "node dist/linear-import.js --file ../../docs/LINEAR_DOCUMENTATION.md --dry-run",
    "dev": "node --import tsx src/linear-import.ts",
    "bun:build": "bun build src/linear-import.ts --outdir dist --target node",
    "bun:import": "bun run src/linear-import.ts",
    "bun:import:docs": "bun run src/linear-import.ts --file ../../docs/LINEAR_DOCUMENTATION.md",
    "bun:import:dry-run": "bun run src/linear-import.ts --file ../../docs/LINEAR_DOCUMENTATION.md --dry-run"
  },
  "keywords": [
    "linear",
    "documentation",
    "import",
    "project-management"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@linear/sdk": "^37.0.0",
    "dotenv": "^16.5.0",
    "marked": "^15.0.0",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
