{
  "name": "Test Chunked Compilation",
  "version": "1.0.0",
  "description": "Test configuration for chunked parallel compilation",
  "license": "MIT",
  "chunking": {
    "enabled": true,
    "strategy": "source",
    "maxParallel": 4
  },
  "sources": [
    {
      "name": "Source 1",
      "type": "inline",
      "content": [
        "! Test source 1",
        "||ads1.example.com^",
        "||tracker1.example.net^"
      ]
    },
    {
      "name": "Source 2",
      "type": "inline",
      "content": [
        "! Test source 2",
        "||ads2.example.com^",
        "||tracker2.example.net^"
      ]
    },
    {
      "name": "Source 3",
      "type": "inline",
      "content": [
        "! Test source 3",
        "||ads3.example.com^",
        "||tracker3.example.net^"
      ]
    },
    {
      "name": "Source 4",
      "type": "inline",
      "content": [
        "! Test source 4",
        "||ads4.example.com^",
        "||tracker4.example.net^"
      ]
    }
  ],
  "transformations": [
    "RemoveComments",
    "Compress",
    "Validate"
  ]
}
