{
  "$schema": "https://raw.githubusercontent.com/AdguardTeam/HostlistCompiler/master/schemas/configuration.schema.json",
  "name": "AdGuard User Filter",
  "description": "Custom filter list compiled from multiple sources",
  "homepage": "https://github.com/jaypatrick/ad-blocking",
  "license": "MIT",
  "version": "1.0.0",
  "output": {
    "path": "data/output/adguard_user_filter.txt",
    "conflictStrategy": "rename"
  },
  "hashVerification": {
    "mode": "warning",
    "requireHashesForRemote": false,
    "failOnMismatch": false,
    "hashDatabasePath": "data/input/.hashes.json"
  },
  "archiving": {
    "enabled": true,
    "mode": "automatic",
    "retentionDays": 90
  },
  "sources": [
    {
      "name": "Local User Rules",
      "source": "data/output/adguard_user_filter.txt",
      "type": "adblock",
      "transformations": [],
      "inclusions": [],
      "exclusions": []
    }
  ],
  "transformations": [
    "Validate",
    "Deduplicate",
    "RemoveEmptyLines",
    "TrimLines",
    "InsertFinalNewLine"
  ],
  "inclusions": [],
  "inclusions_sources": [],
  "exclusions": [],
  "exclusions_sources": []
}
