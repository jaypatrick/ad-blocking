{
  "name": "adguard-api-typescript",
  "version": "1.0.0",
  "description": "TypeScript SDK for AdGuard DNS API v1.11 with feature parity to adguard-api-dotnet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "adguard-dns": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "lint": "eslint src/",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist coverage",
    "bun:build": "bun build src/cli/index.ts --outdir dist/cli --target node",
    "bun:start": "bun dist/cli/index.js",
    "bun:dev": "bun run src/cli/index.ts",
    "bun:lint": "bun run lint",
    "bun:test": "bun test",
    "bun:test:coverage": "bun test --coverage",
    "bun:test:watch": "bun test --watch"
  },
  "keywords": [
    "adguard",
    "dns",
    "api",
    "sdk",
    "client",
    "ad-blocking",
    "typescript"
  ],
  "author": "jaypatrick",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "axios-retry": "^4.0.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "cli-table3": "^0.6.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
