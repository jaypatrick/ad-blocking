{
  "name": "adguard-api-typescript",
  "version": "1.0.0",
  "description": "TypeScript SDK for AdGuard DNS API v1.11 with feature parity to adguard-api-dotnet",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "adguard-dns": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "node --import tsx src/cli/index.ts",
    "lint": "eslint src/",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "adguard",
    "dns",
    "api",
    "sdk",
    "client",
    "ad-blocking",
    "typescript"
  ],
  "author": "jaypatrick",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.0",
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.33.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "chalk": "^5.4.0",
    "commander": "^13.1.0",
    "@inquirer/prompts": "^7.4.0",
    "ora": "^8.2.0",
    "cli-table3": "^0.6.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
